<div class="row">
<div class="col-md-12">
<br><br><br><br>
<div class="table-responsive">
<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>

  <tbody>
    <% @review.each do |review| %>
      <tr>
        <td><%= link_to review.title, review %></td>
        <td><%= review.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
<br>
